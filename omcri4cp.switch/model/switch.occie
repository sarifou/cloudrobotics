<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="switch" scheme="http://cristal.org/omcri4cp/switch#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <kinds name="Switch" scheme="http://cristal.org/omcri4cp/switch#">
    <attributes name="user" mutable="true" required="true" default="root">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="ip" mutable="true" required="true" default="127.0.0.1">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="port" mutable="true" required="true" default="9090">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="dcc.address" mutable="true" required="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="switch.state" mutable="true" default="False">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="switch.id" mutable="true" required="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <actions name="switch" scheme="http://cristal.org/omcri4cp/switch/switch/action#"/>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
  </kinds>
</occi:Extension>
