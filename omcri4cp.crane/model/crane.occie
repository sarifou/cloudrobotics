<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="Crane" scheme="http://cristal.org/omcri4cp/crane#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <kinds name="Crane" scheme="http://cristal.org/omcri4cp/crane#">
    <attributes name="user" mutable="true" required="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="address" mutable="true" required="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
  </kinds>
  <mixins name="Motion" scheme="http://cristal.org/omcri4cp/crane#" applies="//@kinds[term='crane']">
    <attributes name="speed" mutable="true">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="duration" mutable="true">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <actions name="move_forward" scheme="http://cristal.org/omcri4cp/crane/motion/action#"/>
    <actions name="move_backward" scheme="http://cristal.org/omcri4cp/crane/motion/action#"/>
    <actions name="move_up" scheme="http://cristal.org/omcri4cp/crane/motion/action#"/>
    <actions name="move_down" scheme="http://cristal.org/omcri4cp/crane/motion/action#"/>
  </mixins>
  <mixins name="Gripper" scheme="http://cristal.org/omcri4cp/crane#" applies="//@kinds[term='crane']">
    <attributes name="state" mutable="true" default="Open" type="//@types.0"/>
    <actions name="open" scheme="http://cristal.org/omcri4cp/crane/mixin2/action#"/>
    <actions name="close" scheme="http://cristal.org/omcri4cp/crane/mixin2/action#"/>
  </mixins>
  <types xsi:type="occi:EnumerationType" name="State">
    <literals name="Open"/>
    <literals name="Closed"/>
  </types>
</occi:Extension>
