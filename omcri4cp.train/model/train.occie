<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="train" scheme="http://cristal.org/omcri4cp/train#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <kinds name="Train" scheme="http://cristal.org/omcri4cp/train#">
    <attributes name="user" mutable="true" required="true" default="root">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="ip" mutable="true" required="true" default="127.0.0.1">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="port" mutable="true" required="true" default="9090">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="light.state" mutable="true" default="false">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="honk.state" mutable="true" default="false">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="wagon.noise" mutable="true" default="false">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="train.name" mutable="true" required="true" default="DCC0">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="train.noise" mutable="true" default="false">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="train.number" mutable="true" required="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="connection.state" mutable="true" default="false">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="train.speed" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <actions name="faster" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="slower" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="reverse" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="setSpeed" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="stop" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="set_honk" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="set_wagon_noise" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="set_train_noise" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="set_light" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="connect" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <actions name="disconnect" scheme="http://cristal.org/omcri4cp/train/train/action#"/>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
  </kinds>
</occi:Extension>
