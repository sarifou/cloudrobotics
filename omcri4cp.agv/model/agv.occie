<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="agv" scheme="http://cristal.org/omcri4cp/agv#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <kinds name="AGV" scheme="http://cristal.org/omcri4cp/agv#">
    <attributes name="user" mutable="true" required="true" default="root">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="ip" mutable="true" required="true" default="127.0.0.1">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="port" mutable="true" required="true" default="9090">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="connection.state" mutable="true" default="false">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="pose.x" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="pose.y" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="pose.angular" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="speed.linear" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="speed.angular" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="gripper.state" mutable="true" default="false">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
    </attributes>
    <attributes name="gripper.x" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="gripper.y" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="gripper.z" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="distance" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="line.right" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="line.middle" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="line.left" mutable="true" default="0">
      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <actions name="connect" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="disconnect" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="get_distance" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="get_line_sensors" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="forward" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="backward" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="right" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="left" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="right_up" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="right_bottom" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="left_up" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="left_bottom" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="turn_left" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="turn_right" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="turn_around" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="stop" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="open_gripper" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="close_gripper" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="go_to_position" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <actions name="set_gripper_position" scheme="http://cristal.org/omcri4cp/agv/agv/action#"/>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
  </kinds>
</occi:Extension>
