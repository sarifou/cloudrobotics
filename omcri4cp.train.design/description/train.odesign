<toolSections name="Train resources">
		<ownedTools xsi:type="tool:ContainerCreationDescription" 
					name="Create Train"
					containerMappings="//@ownedViewpoints[name='TrainConfiguration']/@ownedRepresentations[name='TrainConfiguration%20Diagram']/@defaultLayer/@containerMappings[name='TrainContainer'] 
		"> 
		            <variable name="container"/>
		            <viewVariable name="containerView"/>
		            <initialOperation>
		              <firstModelOperations xsi:type="tool_1:Switch">
		                <cases conditionExpression="aql:container.oclIsKindOf(occi::Configuration)">
		                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="train::Train" referenceName="resources">
		                    <subModelOperations xsi:type="tool_1:SetValue" featureName="title" valueExpression="['train' + container.oclAsType(Configuration).resources->size().toString()/]"/>
		                  </subModelOperations>
		                </cases>
		                <default/>
		              </firstModelOperations>
		            </initialOperation>
		          </ownedTools>
</toolSections>
<toolSections name="Train links">
</toolSections>   
<toolSections name="Train mixins">
	          <ownedTools xsi:type="tool:ContainerCreationDescription" name="Create Motion" containerMappings="
	//@ownedViewpoints[name='TrainConfiguration']/@ownedRepresentations[name='TrainConfiguration%20Diagram']/@defaultLayer/@containerMappings[name='TrainContainer']/@subContainerMappings[name='MotionMixin']
	">
	            <variable name="container"/>
	            <viewVariable name="containerView"/>
	            <initialOperation>
	              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
	                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="train::Motion" referenceName="parts"/>
	              </firstModelOperations>
	            </initialOperation>
	          </ownedTools>
	          <ownedTools xsi:type="tool:ContainerCreationDescription" name="Create Accessories" containerMappings="
	//@ownedViewpoints[name='TrainConfiguration']/@ownedRepresentations[name='TrainConfiguration%20Diagram']/@defaultLayer/@containerMappings[name='TrainContainer']/@subContainerMappings[name='AccessoriesMixin']
	">
	            <variable name="container"/>
	            <viewVariable name="containerView"/>
	            <initialOperation>
	              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
	                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="train::Accessories" referenceName="parts"/>
	              </firstModelOperations>
	            </initialOperation>
	          </ownedTools>
</toolSections>    
